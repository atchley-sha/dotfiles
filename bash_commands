#Add custom bash commands to be sourced in bashrc

#create BEAM jar and move to convenient directory
alias beamjar='cd ~/BEAM && gradle shadowjar && \
	  cp ~/BEAM/build/libs/beam.sim.RunBeam.jar ~/beam_scenarios/beam.jar'


#function for moving up
function up( )
{
	LIMIT=$1
	P=$PWD
	for ((i=1; i <= LIMIT; i++))
		do
			P=$P/..
		done
	cd $P
}
